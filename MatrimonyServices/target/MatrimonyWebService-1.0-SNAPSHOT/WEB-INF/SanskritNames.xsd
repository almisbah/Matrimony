<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:en="http://mycompany.com/Sanskrit/schemas"
           elementFormDefault="qualified"
           targetNamespace="http://mycompany.com/Sanskrit/schemas">
    <xs:element name="SanskritNameCatagoryRequest">
        <xs:complexType>
            <xs:all>                
                <xs:element name="catagoryName" type="xs:string"/>
            </xs:all>
        </xs:complexType>
    </xs:element>
	<xs:element name="SanskritNameCatagoryResponse">
		<xs:complexType>
			<xs:all>
				<xs:element name="catagoryList" type="en:CatagoryType"/>
			</xs:all>
		</xs:complexType>
	</xs:element>

    <xs:element name="SaveSanskritNameRequest">
        <xs:complexType>
            <xs:all> 
            	<xs:element name="parentCatagoryName" type="xs:string"/>               
                <xs:element name="catagoryName" type="xs:string"/>
                <xs:element name="englishName" type="xs:string"></xs:element>
                <xs:element name="sanskritName" type="xs:string"></xs:element>
                <xs:element name="imageName" type="xs:string"></xs:element>
                <xs:element name="description" type="xs:string"/>
            </xs:all>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="SaveSanskritNameResponse">
		<xs:complexType>
			<xs:all>
				<xs:element name="response" type="en:GenericResponseType"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
    
    <xs:complexType name="GenericResponseType">
        <xs:sequence>
            <xs:element name="desc" type="xs:date"/>
            <xs:element name="respCode" type="xs:date"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="CatagoryType">
        <xs:sequence>
            <xs:element name="name" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    
    
</xs:schema>
